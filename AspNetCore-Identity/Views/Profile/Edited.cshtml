@model Identity.Service.Application.Commands.User.EditUserProfileCommand

@{
    ViewData["Title"] = "Profile Updated";
}

@section form_elements_scripts {
    <!--form-elements.js - Page Specific JS codes-->
    <script type="text/javascript" src="~/js/scripts/form-elements.js"></script>
}

@section Breadcrumbs
    {
    <div class="row">
        <div class="col s10 m6 l6">
            <ol class="breadcrumbs">
                <li><a asp-action="Edit">Profile</a></li>
                <li class="active">Edited</li>
            </ol>
        </div>
    </div>
}
<div class="divider"></div>
<div class="row">
    <div class="col s12">
        <div class="card-panel">
            <h4 class="header2">Profile Updated</h4>
            <div class="row">
                <div class="col s12" asp-action="Edit" method="post">
                    <div class="row">
                        <div class="input-field col s12 l4">
                            <span><b>Username</b> : @Model.Username</span>
                        </div>
                        <div class="input-field col s12 l4">
                            <span><b>Email</b> : @Model.Email</span>
                        </div>
                        <div class="input-field col s12 l4">
                            <span><b>Phone number</b> : @Model.PhoneNumber</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>